						  </tr>
					  </thead>   
					  <tbody>
					  <!-- This is the table where we put the report entries -->
					  {{#each reports}}
						<tr>
							<td>{{this.name}}</td>
							<td class="center">{{this.date}}</td>
							<!-- td class="center"><span class="label label-important">High</span></td-->
							<td class="center">{{this.severity}}</td>
							<td class="center">{{this.num_alarms}}</td>
							<td class="center">{{this.tags}}</td>
							<td class="center">
								<a class="btn btn-success" href="chart?id={{this.id}}">
